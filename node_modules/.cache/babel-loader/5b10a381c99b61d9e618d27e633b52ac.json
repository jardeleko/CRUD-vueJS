{"ast":null,"code":"import { ContactService } from '../../services/ContactService';\nexport default {\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: 'Edit',\n  data: function () {\n    return {\n      userID: this.$route.params.id,\n      loading: false,\n      errorMessage: null,\n      contact: {},\n      group: []\n    };\n  },\n  created: async function () {\n    try {\n      this.loading = true;\n      let res = await ContactService.getContact(this.userID);\n      let groupQuery = await ContactService.getAllGroups();\n      this.groups = groupQuery.data;\n      this.contact = res.data;\n      this.loading = false;\n    } catch (err) {\n      this.errorMessage = err;\n    }\n  },\n  methods: {\n    updateUser: async function () {\n      try {\n        let res = await ContactService.putContact(this.contact, this.userID);\n\n        if (res) {\n          return this.$router.push('/');\n        } else {\n          return this.$router.push(`/contact/edit/${this.userID}`);\n        }\n      } catch (err) {\n        console.log(err);\n      }\n    },\n    isNext: function () {\n      return Object.keys(this.contact).length > 0 && Object.keys(this.group).length > 0;\n    }\n  }\n};","map":{"version":3,"mappings":"AAmDA,SAASA,cAAT,QAA+B,+BAA/B;AAEA,eAAe;EACb;EACAC,IAAI,EAAE,MAFO;EAGbC,IAAI,EAAE,YAAY;IAChB,OAAO;MACHC,MAAM,EAAE,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,EADxB;MAEHC,OAAO,EAAE,KAFN;MAGHC,YAAY,EAAE,IAHX;MAIHC,OAAO,EAAE,EAJN;MAKHC,KAAK,EAAE;IALJ,CAAP;EAOD,CAXY;EAYbC,OAAO,EAAE,kBAAkB;IACzB,IAAI;MACF,KAAKJ,OAAL,GAAe,IAAf;MACA,IAAIK,GAAE,GAAI,MAAMZ,cAAc,CAACa,UAAf,CAA0B,KAAKV,MAA/B,CAAhB;MACA,IAAIW,UAAS,GAAI,MAAMd,cAAc,CAACe,YAAf,EAAvB;MACA,KAAKC,MAAL,GAAcF,UAAU,CAACZ,IAAzB;MACA,KAAKO,OAAL,GAAeG,GAAG,CAACV,IAAnB;MACA,KAAKK,OAAL,GAAe,KAAf;IACF,CAPA,CAQA,OAAOU,GAAP,EAAY;MACV,KAAKT,YAAL,GAAoBS,GAApB;IACF;EACD,CAxBY;EAyBbC,OAAO,EAAE;IACPC,UAAU,EAAE,kBAAgB;MAC1B,IAAG;QACD,IAAIP,GAAE,GAAI,MAAMZ,cAAc,CAACoB,UAAf,CAA0B,KAAKX,OAA/B,EAAwC,KAAKN,MAA7C,CAAhB;;QACA